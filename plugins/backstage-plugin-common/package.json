{
  "name": "@pagerduty/backstage-plugin-common",
  "description": "Common components for PagerDuty plugins for Backstage",
  "version": "0.2.3",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "types": "dist/index.d.ts"
  },
  "backstage": {
    "pluginId": "pagerduty",
    "role": "common-library",
    "pluginPackages": [
      "@pagerduty/backstage-plugin",
      "@pagerduty/backstage-plugin-common",
      "@pagerduty/backstage-plugin-backend"
    ]
  },
  "homepage": "https://pagerduty.github.io/backstage-plugin-docs/index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/PagerDuty/backstage-plugins",
    "directory": "plugins/backstage-plugin-common"
  },
  "keywords": [
    "backstage",
    "plugin",
    "pagerduty"
  ],
  "scripts": {
    "build": "yarn tsc && backstage-cli package build",
    "lint": "backstage-cli package lint",
    "test": "backstage-cli package test",
    "prepack": "backstage-cli package prepack",
    "postpack": "backstage-cli package postpack",
    "clean": "backstage-cli package clean"
  },
  "devDependencies": {
    "@backstage/cli": "^0.25.1",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist"
  ]
}
