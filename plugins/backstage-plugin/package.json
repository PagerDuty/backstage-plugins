{
  "name": "@pagerduty/backstage-plugin",
  "description": "A Backstage plugin that integrates towards PagerDuty",
  "version": "0.16.3",
  "main": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "backstage": {
    "pluginId": "pagerduty",
    "role": "frontend-plugin",
    "pluginPackages": [
      "@pagerduty/backstage-plugin",
      "@pagerduty/backstage-plugin-common",
      "@pagerduty/backstage-plugin-backend"
    ]
  },
  "homepage": "https://github.com/pagerduty/backstage-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/pagerduty/backstage-plugin.git",
    "directory": "."
  },
  "keywords": [
    "backstage",
    "plugin",
    "pagerduty"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "backstage-cli package build && copyfiles -u 1 src/assets/*.svg ./ && cp ../backstage-plugin-common/config.d.ts ./",
    "start": "backstage-cli package start --config ./dev/app-config.yaml",
    "lint": "backstage-cli package lint",
    "test": "backstage-cli package test",
    "prepare": "husky install",
    "prepack": "node ../../scripts/replace-versions.js backstage-plugin && backstage-cli package prepack",
    "postpack": "backstage-cli package postpack",
    "clean": "backstage-cli package clean && rm -f config.d.ts"
  },
  "dependencies": {
    "@backstage/catalog-client": "^1.12.0",
    "@backstage/catalog-model": "^1.7.5",
    "@backstage/core-compat-api": "^0.5.2",
    "@backstage/core-components": "^0.18.0",
    "@backstage/core-plugin-api": "^1.11.0",
    "@backstage/errors": "^1.2.7",
    "@backstage/frontend-plugin-api": "^0.12.0",
    "@backstage/plugin-catalog": "^1.31.3",
    "@backstage/plugin-catalog-react": "^1.21.0",
    "@backstage/plugin-home-react": "^0.1.30",
    "@backstage/theme": "^0.5.6",
    "@backstage/ui": "^0.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.61",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.19",
    "@mui/x-date-pickers": "^7.6.1",
    "@pagerduty/backstage-plugin-common": "workspace:~",
    "@tanstack/react-query": "^5.40.1",
    "classnames": "^2.2.6",
    "luxon": "^3.4.1",
    "material-react-table": "^2.13.0",
    "react-use": "^17.2.4",
    "uuid": "^10.0.0",
    "validate-color": "^2.2.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^20.0.0",
    "react-dom": "^18.0.0 || ^20.0.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@backstage/cli": "^0.34.3",
    "@backstage/core-app-api": "^1.19.0",
    "@backstage/dev-utils": "^1.1.14",
    "@backstage/frontend-defaults": "^0.3.1",
    "@backstage/test-utils": "^1.7.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/copyfiles": "^2",
    "copyfiles": "^2.4.1"
  },
  "files": [
    "dist/**/*",
    "config.d.ts",
    "assets/**/*"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha/index.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "configSchema": "config.d.ts",
  "module": "./dist/index.esm.js"
}